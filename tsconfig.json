{
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/__mocks__/**/*.ts"
    ],
   "compilerOptions": {
        "target": "ES2017",
        "module": "ESNext",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "skipLibCheck": true
   }
}
